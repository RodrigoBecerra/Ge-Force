CREATE TABLE `Horario` (
  `Id_Hor`  int AUTO_INCREMENT NOT NULL,
  `Id_Pl`   int(5) NOT NULL,
  `Hora`    varchar(15) NOT NULL,
  `Sala`    varchar(15) NOT NULL,
  `Dia`     date NOT NULL,
  `Repitencia`       varchar(2) NOT NULL,
  PRIMARY KEY (`Id_Hor`),
  FOREIGN KEY (`Id_Pl`) REFERENCES Plan(`Id_Pl`)
) ENGINE = InnoDB;
